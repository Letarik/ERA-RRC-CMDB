@model IEnumerable<RCMD.Models.Particle_Size_Distribution>

@{
    ViewBag.Title = "Index";
}

<p>
    <h4>
        Particle Size Distribution Page | @Html.ActionLink("Create New Particle Size Distribution", "Create")
    </h4>
    <hr />
</p>
<table class="table table-striped table-bordered table-condensed">
    <tbody>
        <tr>
            <td align="center">
                <div class="container-narrow">
                    <div class="col-md-13">
                        @Html.DisplayNameFor(model => model.Sample_Preparation)
                    </div>
                </div>
            </td>
            <td align="center">
                <div class="container-narrow">
                    <div class="col-md-13">
                        @Html.DisplayNameFor(model => model.Method_Of_Sieving)
                    </div>
                </div>
            </td>
            <td align="center">
                <div class="container-narrow">
                    <div class="col-md-13">
                        @Html.DisplayNameFor(model => model.Sieve_Size)
                    </div>
                </div>
            </td>
            <td align="center">
                <div class="container-narrow">
                    <div class="col-md-13">
                        @Html.DisplayNameFor(model => model.Weight_Retained)
                    </div>
                </div>
            </td>
            <td align="center">
                <div class="container-narrow">
                    <div class="col-md-13">
                        @Html.DisplayNameFor(model => model.Retained_Percent)
                    </div>
                </div>
            </td>
            <td align="center">
                <div class="container-narrow">
                    <div class="col-md-13">
                        @Html.DisplayNameFor(model => model.Passing_Percent)
                    </div>
                </div>
            </td>
            <td align="center">
                <div class="container-narrow">
                    <div class="col-md-13">
                        @Html.DisplayNameFor(model => model.Note)
                    </div>
                </div>
            </td>
            <td align="center">
                <div class="container-narrow">
                    <div class="col-md-13">
                        @Html.DisplayNameFor(model => model.Remark)
                    </div>
                </div>
            </td>
            <td align="center">
                <div class="container-narrow">
                    <div class="col-md-13">
                        @Html.DisplayNameFor(model => model.Material_Description.Project_Name)
                    </div>
                </div>
            </td>
            <td align="center">
                <div class="container-narrow">
                    <div class="col-md-13">
                        @Html.DisplayNameFor(model => model.Material_Description1.Project_Name)
                    </div>
                </div>
            </td>
            <td colspan="3" align="center">
                <div class="container-narrow">
                    <div class="col-md-13">
                        <b> Alternate Actions</b>
                    </div>
                </div>
            </td>
        </tr>



        @foreach (var item in Model)
        {
            <tr>
                <td>
                    <div class="container-narrow">
                        <div class="col-md-13">
                            @Html.DisplayFor(modelItem => item.Sample_Preparation)
                        </div>
                    </div>
                </td>
                <td>
                    <div class="container-narrow">
                        <div class="col-md-13">
                            @Html.DisplayFor(modelItem => item.Method_Of_Sieving)
                        </div>
                    </div>
                </td>
                <td>
                    <div class="container-narrow">
                        <div class="col-md-13">
                            @Html.DisplayFor(modelItem => item.Sieve_Size)
                        </div>
                    </div>
                </td>
                <td>
                    <div class="container-narrow">
                        <div class="col-md-13">
                            @Html.DisplayFor(modelItem => item.Weight_Retained)
                        </div>
                    </div>
                </td>
                <td>
                    <div class="container-narrow">
                        <div class="col-md-13">
                            @Html.DisplayFor(modelItem => item.Retained_Percent)
                        </div>
                    </div>
                </td>
                <td>
                    <div class="container-narrow">
                        <div class="col-md-13">
                            @Html.DisplayFor(modelItem => item.Passing_Percent)
                        </div>
                    </div>
                </td>
                <td>
                    <div class="container-narrow">
                        <div class="col-md-13">
                            @Html.DisplayFor(modelItem => item.Note)
                        </div>
                    </div>
                </td>
                <td>
                    <div class="container-narrow">
                        <div class="col-md-13">
                            @Html.DisplayFor(modelItem => item.Remark)
                        </div>
                    </div>
                </td>
                <td>
                    <div class="container-narrow">
                        <div class="col-md-13">
                            @Html.DisplayFor(modelItem => item.Material_Description.Project_Name)
                        </div>
                    </div>
                </td>
                <td>
                    <div class="container-narrow">
                        <div class="col-md-13">
                            @Html.DisplayFor(modelItem => item.Material_Description1.Project_Name)
                        </div>
                    </div>
                </td>



                <td>
                    <div class="container-narrow">
                        <div class="col-md-13">
                            <input type="submit" value="Edit" class="btn btn-warning col-lg-3 col-lg-offset-0" style="width:70px; height:35px;" onclick="location.href='@Url.Action("Edit",new {id = item.ID})'" />
                            @*@Html.ActionLink("Edit", "Edit", new { id = item.ID})*@
                        </div>
                    </div>
                </td>
                <td>
                    <div class="container-narrow">
                        <div class="col-md-13">
                            <input type="submit" value="Details" class="btn btn-success col-lg-3 col-lg-offset-0" style="width:70px; height:35px;" onclick="location.href='@Url.Action("Details",new {id = item.ID})'" />
                            @*@Html.ActionLink("Details", "Details", new { id = item.ID })*@
                        </div>
                    </div>
                </td>
                <td>
                    <div class="container-narrow">
                        <div class="col-md-13">
                            <input type="submit" value="Delete" class="btn btn-danger col-lg-3 col-lg-offset-0" style="width:70px; height:35px;" onclick="location.href='@Url.Action("Delete",new {id = item.ID})'" />
                            @*@Html.ActionLink("Delete", "Delete", new { id = item.ID })*@
                        </div>
                    </div>
                </td>

            </tr>

        }

    </tbody>
</table>
