@model IEnumerable<RCMD.Models.Density_Determination>

@{
    ViewBag.Title = "Index";
}

<h4>
    Density Determination Page | @Html.ActionLink("Create New Density Determination", "Create")
</h4>
<hr />
@*<p>
        @Html.ActionLink("Create New", "Create")
    </p>*@
<table class="table table-striped table-bordered table-condensed">
    <tr>
        <td align="center">
            <div class="container-narrow">
                <div class="col-md-13">
                    @Html.DisplayNameFor(model => model.Soaking_Condition)
                </div>
            </div>
        </td>
        <td align="center">
            <div class="container-narrow">
                <div class="col-md-13">
                    @Html.DisplayNameFor(model => model.Model_Type)
                </div>
            </div>
        </td>
        <td align="center">
            <div class="container-narrow">
                <div class="col-md-13">
                    @Html.DisplayNameFor(model => model.Model_No)
                </div>
            </div>
        </td>
        <td align="center">
            <div class="container-narrow">
                <div class="col-md-13">
                    @Html.DisplayNameFor(model => model.Weight_Of_Soil_Plus_Mold)
                </div>
            </div>
        </td>
        <td align="center">
            <div class="container-narrow">
                <div class="col-md-13">
                    @Html.DisplayNameFor(model => model.Weight_Of_Mold)
                </div>
            </div>
        </td>
        <td align="center">
            <div class="container-narrow">
                <div class="col-md-13">
                    @Html.DisplayNameFor(model => model.Weight_Of_Soil)
                </div>
            </div>
        </td>
        <td align="center">
            <div class="container-narrow">
                <div class="col-md-13">
                    @Html.DisplayNameFor(model => model.Volume_Of_Mold)
                </div>
            </div>
        </td>
        <td align="center">
            <div class="container-narrow">
                <div class="col-md-13">
                    @Html.DisplayNameFor(model => model.Wet_Density_Of_Soil)
                </div>
            </div>
        </td>
        <td align="center">
            <div class="container-narrow">
                <div class="col-md-13">
                    @Html.DisplayNameFor(model => model.Dry_Density_Of_Soil)
                </div>
            </div>
        </td>
        <td align="center">
            <div class="container-narrow">
                <div class="col-md-13">
                    @Html.DisplayNameFor(model => model.Material_Description.Project_Name)
                </div>
            </div>
        </td>
        <td align="center">
            <div class="container-narrow">
                <div class="col-md-13">
                    @Html.DisplayNameFor(model => model.Moisture.Container_No)
                </div>
            </div>
        </td>
        <td colspan="3" align="center">
            <div class="container-narrow">
                <div class="col-md-13">
                    <b> Alternate Actions</b>
                </div>
            </div>
        </td>

    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                <div class="container-narrow">
                    <div class="col-md-13">
                        @Html.DisplayFor(modelItem => item.Soaking_Condition)
                    </div>
                </div>
            </td>
            <td>
                <div class="container-narrow">
                    <div class="col-md-13">
                        @Html.DisplayFor(modelItem => item.Model_Type)
                    </div>
                </div>
            </td>
            <td>
                <div class="container-narrow">
                    <div class="col-md-13">
                        @Html.DisplayFor(modelItem => item.Model_No)
                    </div>
                </div>
            </td>
            <td>
                <div class="container-narrow">
                    <div class="col-md-13">
                        @Html.DisplayFor(modelItem => item.Weight_Of_Soil_Plus_Mold)
                    </div>
                </div>
            </td>
            <td>
                <div class="container-narrow">
                    <div class="col-md-13">
                        @Html.DisplayFor(modelItem => item.Weight_Of_Mold)
                    </div>
                </div>
            </td>
            <td>
                <div class="container-narrow">
                    <div class="col-md-13">
                        @Html.DisplayFor(modelItem => item.Weight_Of_Soil)
                    </div>
                </div>
            </td>
            <td>
                <div class="container-narrow">
                    <div class="col-md-13">
                        @Html.DisplayFor(modelItem => item.Volume_Of_Mold)
                    </div>
                </div>
            </td>
            <td>
                <div class="container-narrow">
                    <div class="col-md-13">
                        @Html.DisplayFor(modelItem => item.Wet_Density_Of_Soil)
                    </div>
                </div>
            </td>
            <td>
                <div class="container-narrow">
                    <div class="col-md-13">
                        @Html.DisplayFor(modelItem => item.Dry_Density_Of_Soil)
                    </div>
                </div>
            </td>
            <td>
                <div class="container-narrow">
                    <div class="col-md-13">
                        @Html.DisplayFor(modelItem => item.Material_Description.Project_Name)
                    </div>
                </div>
            </td>
            <td>
                <div class="container-narrow">
                    <div class="col-md-13">
                        @Html.DisplayFor(modelItem => item.Moisture.Container_No)
                    </div>
                </div>
            </td>

            <td>
                <div class="container-narrow">
                    <div class="col-md-13">
                        <input type="submit" value="Edit" class="btn btn-warning col-lg-3 col-lg-offset-0" style="width:70px; height:35px;" onclick="location.href='@Url.Action("Edit",new {id = item.ID})'" />
                        @*@Html.ActionLink("Edit", "Edit", new { id = item.ID})*@
                    </div>
                </div>
            <td>
                <div class="container-narrow">
                    <div class="col-md-13">
                        <input type="submit" value="Details" class="btn btn-success col-lg-3 col-lg-offset-0" style="width:70px; height:35px;" onclick="location.href='@Url.Action("Details",new {id = item.ID})'" />
                        @*@Html.ActionLink("Details", "Details", new { id = item.ID })*@
                    </div>
                </div>
            </td>
            <td>
                <div class="container-narrow">
                    <div class="col-md-13">
                        <input type="submit" value="Delete" class="btn btn-danger col-lg-3 col-lg-offset-0" style="width:70px; height:35px;" onclick="location.href='@Url.Action("Delete",new {id = item.ID})'" />
                        @*@Html.ActionLink("Delete", "Delete", new { id = item.ID })*@
                    </div>
                </div>
            </td>
            @*@Html.ActionLink("Edit", "Edit", new { id=item.ID }) |
                @Html.ActionLink("Details", "Details", new { id=item.ID }) |
                @Html.ActionLink("Delete", "Delete", new { id=item.ID })*@
            </td>
        </tr>
    }

</table>
















