@model IEnumerable<RCMD.Models.Penetration_Test_Data>

@{
    ViewBag.Title = "Index";
}

<p>
    <h4>
        Penetration Test Data Page | @Html.ActionLink("Create New Penetration Test Data", "Create")
    </h4>
    <hr />
</p>
<table class="table table-striped table-bordered table-condensed">
    <tbody>
        <tr>
            <td align="center">
                <div class="container-narrow">
                    <div class="col-md-13">
                        @Html.DisplayNameFor(model => model.Ring_Factor)
                    </div>
                </div>
            </td>
            <td align="center">
                <div class="container-narrow">
                    <div class="col-md-13">
                        @Html.DisplayNameFor(model => model.Penetration)
                    </div>
                </div>
            </td>
            <td align="center">
                <div class="container-narrow">
                    <div class="col-md-13">
                        @Html.DisplayNameFor(model => model.No_Of_Blows)
                    </div>
                </div>
            </td>
            <td align="center">
                <div class="container-narrow">
                    <div class="col-md-13">
                        @Html.DisplayNameFor(model => model.Dialrdg)
                    </div>
                </div>
            </td>
            <td align="center">
                <div class="container-narrow">
                    <div class="col-md-13">
                        @Html.DisplayNameFor(model => model.Penetration_Load)
                    </div>
                </div>
            </td>

            <td align="center">
                <div class="container-narrow">
                    <div class="col-md-13">
                        @Html.DisplayNameFor(model => model.Cor_Load)
                    </div>
                </div>
            </td>
            <td align="center">
                <div class="container-narrow">
                    <div class="col-md-13">
                        @Html.DisplayNameFor(model => model.CBR)
                    </div>
                </div>
            </td>
            <td align="center">
                <div class="container-narrow">
                    <div class="col-md-13">
                        @Html.DisplayNameFor(model => model.RDG_Before_Soaking)
                    </div>
                </div>
            </td>
            <td align="center">
                <div class="container-narrow">
                    <div class="col-md-13">
                        @Html.DisplayNameFor(model => model.RDG_After_Soaking)
                    </div>
                </div>
            </td>



            <td align="center">
                <div class="container-narrow">
                    <div class="col-md-13">
                        @Html.DisplayNameFor(model => model.Percent_Swell)
                    </div>
                </div>
            </td>
            <td align="center">
                <div class="container-narrow">
                    <div class="col-md-13">
                        @Html.DisplayNameFor(model => model.Average_Percent_Swell)
                    </div>
                </div>
            </td>
            <td align="center">
                <div class="container-narrow">
                    <div class="col-md-13">
                        @Html.DisplayNameFor(model => model.Unit)
                    </div>
                </div>
            </td>
            <td align="center">
                <div class="container-narrow">
                    <div class="col-md-13">
                        @Html.DisplayNameFor(model => model.Blow_No)
                    </div>
                </div>
            </td>
            <td align="center">
                <div class="container-narrow">
                    <div class="col-md-13">
                        @Html.DisplayNameFor(model => model.Material_Description.Project_Name)
                    </div>
                </div>
            </td>


            <td colspan="3" align="center">
                <div class="container-narrow">
                    <div class="col-md-13">
                        <b> Alternate Actions</b>
                    </div>
                </div>
            </td>
        </tr>

        @foreach (var item in Model)
        {
            <tr>
                <td>
                    <div class="container-narrow">
                        <div class="col-md-13">
                            @Html.DisplayFor(modelItem => item.Ring_Factor)
                        </div>
                    </div>
                </td>
                <td>
                    <div class="container-narrow">
                        <div class="col-md-13">
                            @Html.DisplayFor(modelItem => item.Penetration)
                        </div>
                    </div>
                </td>
                <td>
                    <div class="container-narrow">
                        <div class="col-md-13">
                            @Html.DisplayFor(modelItem => item.No_Of_Blows)
                        </div>
                    </div>
                </td>
                <td>
                    <div class="container-narrow">
                        <div class="col-md-13">
                            @Html.DisplayFor(modelItem => item.Dialrdg)
                        </div>
                    </div>
                </td>
                <td>
                    <div class="container-narrow">
                        <div class="col-md-13">
                            @Html.DisplayFor(modelItem => item.Penetration_Load)
                        </div>
                    </div>
                </td>
                <td>
                    <div class="container-narrow">
                        <div class="col-md-13">
                            @Html.DisplayFor(modelItem => item.Cor_Load)
                        </div>
                    </div>
                </td>
                <td>
                    <div class="container-narrow">
                        <div class="col-md-13">
                            @Html.DisplayFor(modelItem => item.CBR)
                        </div>
                    </div>
                </td>
                <td>
                    <div class="container-narrow">
                        <div class="col-md-13">
                            @Html.DisplayFor(modelItem => item.RDG_Before_Soaking)
                        </div>
                    </div>
                </td>
                <td>
                    <div class="container-narrow">
                        <div class="col-md-13">
                            @Html.DisplayFor(modelItem => item.RDG_After_Soaking)
                        </div>
                    </div>
                </td>
                <td>
                    <div class="container-narrow">
                        <div class="col-md-13">
                            @Html.DisplayFor(modelItem => item.Percent_Swell)
                        </div>
                    </div>
                </td>







                <td>
                    <div class="container-narrow">
                        <div class="col-md-13">
                            @Html.DisplayFor(modelItem => item.Average_Percent_Swell)
                        </div>
                    </div>
                </td>
                <td>
                    <div class="container-narrow">
                        <div class="col-md-13">
                            @Html.DisplayFor(modelItem => item.Unit)
                        </div>
                    </div>
                </td>
                <td>
                    <div class="container-narrow">
                        <div class="col-md-13">
                            @Html.DisplayFor(modelItem => item.Blow_No)
                        </div>
                    </div>
                </td>
                <td>
                    <div class="container-narrow">
                        <div class="col-md-13">
                            @Html.DisplayFor(modelItem => item.Material_Description.Project_Name)
                        </div>
                    </div>
                </td>


                <td>
                    <div class="container-narrow">
                        <div class="col-md-13">
                            <input type="submit" value="Edit" class="btn btn-warning col-lg-3 col-lg-offset-0" style="width:70px; height:35px;" onclick="location.href='@Url.Action("Edit",new {id = item.ID})'" />
                            @*@Html.ActionLink("Edit", "Edit", new { id = item.ID})*@
                        </div>
                    </div>
                </td>
                <td>
                    <div class="container-narrow">
                        <div class="col-md-13">
                            <input type="submit" value="Details" class="btn btn-success col-lg-3 col-lg-offset-0" style="width:70px; height:35px;" onclick="location.href='@Url.Action("Details",new {id = item.ID})'" />
                            @*@Html.ActionLink("Details", "Details", new { id = item.ID })*@
                        </div>
                    </div>
                </td>
                <td>
                    <div class="container-narrow">
                        <div class="col-md-13">
                            <input type="submit" value="Delete" class="btn btn-danger col-lg-3 col-lg-offset-0" style="width:70px; height:35px;" onclick="location.href='@Url.Action("Delete",new {id = item.ID})'" />
                            @*@Html.ActionLink("Delete", "Delete", new { id = item.ID })*@
                        </div>
                    </div>
                </td>
            </tr>
        }
    </tbody>
</table>









