@model IEnumerable<RCMD.Models.Moisture>

@{
    ViewBag.Title = "Index";
}

<p>
    <h4>
        Moisture |  @Html.ActionLink("Create New", "Create")
    </h4>
    <hr /> 
</p>
<table class="table table-striped table-bordered table-condensed">
    <tbody>
        <tr>
            <td align="center">
                <div class="container-narrow">
                    <div class="col-md-13">
                        @Html.DisplayNameFor(model => model.Container_No)
                       
                    </div>
                    
                </div>
            </td>
            <td>
                <div class="container-narrow">
                    <div class="col-md-13">
                         @Html.DisplayNameFor(model => model.Wet_Soil_Container)
                    </div>
                </div>
            </td>
            <td>
                <div class="container-narrow">
                    <div class="col-md-13">
                        @Html.DisplayNameFor(model => model.Dry_Soil_Container)
                    </div>    
                </div>
            </td>
            <td>
                <div class="container-narrow">
                    <div class="col-md-13">
                        @Html.DisplayNameFor(model => model.Weight_of_Water)
                    </div>
                </div>
            </td>
            <td>
                <div class="container-narrow">
                    <div class="col-md-13">
                        @Html.DisplayNameFor(model => model.Weight_of_Container)    
                    </div>
                </div>
            </td>
            <td>
                <div class="container-narrow">
                    <div class="col-md-13">
                        @Html.DisplayNameFor(model => model.Weight_of_Dry_Soil)
                    </div>
                </div>
            </td>
            <td>
                <div class="container-narrow">
                    <div class="col-md-13">
                        @Html.DisplayNameFor(model => model.Moisture_Content)
                    </div>
                </div>
            </td>
            <td>
                <div class="container-narrow">
                    <div class="col-md-13">
                        @Html.DisplayNameFor(model => model.OMC)
                    </div>
                </div>
            </td>
            <td>
                <div class="container-narrow">
                    <div class="col-md-13">
                        @Html.DisplayNameFor(model => model.Remark)
                    </div>
                </div>
            </td>
            <td>
                <div class="container-narrow">
                    <div class="col-md-13">
                        @Html.DisplayNameFor(model => model.Density.Remark)
                    </div>
                </div>
            </td>
            <td>
                <div class="container-narrow">
                    <div class="col-md-13">
                        @Html.DisplayNameFor(model => model.Material_Description.Project_Name)
                    </div>
                </div>
            </td>
            <td colspan="3">
            Alternate Actions
            </td>
        </tr>

        @foreach (var item in Model)
        {
            <tr>
                <td>
                    <div class="container-narrow">
                        <div class="col-md-13">
                            @Html.DisplayFor(modelItem => item.Container_No)    
                        </div>
                    </div>
                </td>
                <td>
                    <div class="container-narrow">
                        <div class="col-md-13">
                            @Html.DisplayFor(modelItem => item.Wet_Soil_Container)
                        </div>
                    </div>
                </td>
                <td>
                    <div class="container-narrow">
                        <div class="col-md-13">
                            @Html.DisplayFor(modelItem => item.Dry_Soil_Container)
                        </div>
                    </div>
                </td>
                <td>
                    <div class="container-narrow">
                        <div class="col-md-13">
                            @Html.DisplayFor(modelItem => item.Weight_of_Water)
                        </div>
                    </div>
                </td>
                <td>
                    <div class="container-narrow">
                        <div class="col-md-13">
                            @Html.DisplayFor(modelItem => item.Weight_of_Container)
                        </div>
                    </div>
                </td>
                <td>
                    <div class="container-narrow">
                        <div class="col-md-13">
                            @Html.DisplayFor(modelItem => item.Weight_of_Dry_Soil)
