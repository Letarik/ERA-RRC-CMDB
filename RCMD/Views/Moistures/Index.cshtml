                    </div>
                </td>
                <td>
                    <div class="container-narrow">
                        <div class="col-md-13">
                            @Html.DisplayFor(modelItem => item.Remark)
                        </div>
                    </div>
                </td>
                <td>
                    <div class="container-narrow">
                        <div class="col-md-13">
                            @Html.DisplayFor(modelItem => item.Density.Remark)
                        </div>
                    </div>
                </td>
                <td>
                    <div class="container-narrow">
                        <div class="col-md-13">
                            @Html.DisplayFor(modelItem => item.Material_Description.Project_Name)
                        </div>
                    </div>
                </td>
                <td>
                    <div class="container-narrow">
                        <div class="col-md-13">
                            <input type="submit" value="Edit" class="btn btn-warning col-lg-3 col-lg-offset-0" style="width:70px; height:35px;" onclick="location.href='@Url.Action("Edit",new {id = item.ID})'" />
                            @*@Html.ActionLink("Edit", "Edit", new { id = item.ID})*@
                        </div>
                    </div>
                </td>
                <td>
                    <div class="container-narrow">
                        <div class="col-md-13">
                            <input type="submit" value="Details" class="btn btn-success col-lg-3 col-lg-offset-0" style="width:70px; height:35px;" onclick="location.href='@Url.Action("Details",new {id = item.ID})'" />
                            @*@Html.ActionLink("Details", "Details", new { id = item.ID })*@
                        </div>
                    </div>
                </td>
                <td>
                    <div class="container-narrow">
                        <div class="col-md-13">
                            <input type="submit" value="Delete" class="btn btn-danger col-lg-3 col-lg-offset-0" style="width:70px; height:35px;" onclick="location.href='@Url.Action("Delete",new {id = item.ID})'" />
                            @*@Html.ActionLink("Delete", "Delete", new { id = item.ID })*@
                        </div>
                    </div>
                </td>
            </tr>
        }
    </tbody>   
</table>
