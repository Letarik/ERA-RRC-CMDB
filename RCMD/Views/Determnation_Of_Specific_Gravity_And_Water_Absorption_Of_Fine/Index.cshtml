@model IEnumerable<RCMD.Models.Determnation_Of_Specific_Gravity_And_Water_Absorption_Of_Fine>

    @{
        ViewBag.Title = "Index";
    }

    <p>
        <h4>
            Determnation Of Specific Gravity And Water Absorption Of Fine Page | @Html.ActionLink("Create New Determnation Of Specific Gravity And Water Absorption Of Fine", "Create")
        </h4>
        <hr />
    </p>
    <table class="table table-striped table-bordered table-condensed">
        <tbody>
            <tr>
                <td align="center">
                    <div class="container-narrow">
                        <div class="col-md-13">
                            @Html.DisplayNameFor(model => model.WSASDT)

                        </div>
                    </div>
                </td>
                <td align="center">
                    <div class="container-narrow">
                        <div class="col-md-13">
                            @Html.DisplayNameFor(model => model.WSSDAIA)
                        </div>
                    </div>
                </td>
                <td align="center">
                    <div class="container-narrow">
                        <div class="col-md-13">
                            @Html.DisplayNameFor(model => model.MoAIW)
                        </div>
                    </div>
                </td>
                <td align="center">
                    <div class="container-narrow">
                        <div class="col-md-13">
                            @Html.DisplayNameFor(model => model.Specific_Gravity)
                        </div>
                    </div>
                </td>
                <td align="center">
                    <div class="container-narrow">
                        <div class="col-md-13">
                            @Html.DisplayNameFor(model => model.Apparent_Specific_Gravity)
                        </div>
                    </div>
                </td>
                <td align="center">
                    <div class="container-narrow">
                        <div class="col-md-13">
                            @Html.DisplayNameFor(model => model.Water_Absorption_of_Aggregate)
                        </div>
                    </div>
                </td>
                <td align="center">
                    <div class="container-narrow">
                        <div class="col-md-13">
                            @Html.DisplayNameFor(model => model.Location.District)
                        </div>
                    </div>
                </td>

                <td colspan="3" align="center">
                    <div class="container-narrow">
                        <div class="col-md-13">
                            <b> Alternate Actions</b>
                        </div>
                    </div>
                </td>
            </tr>

            @foreach (var item in Model)
            {
                <tr>
                    <td>
                        <div class="container-narrow">
                            <div class="col-md-13">


                                @Html.DisplayFor(modelItem => item.WSASDT)

                            </div>
                        </div>
                    </td>
                    <td>
                        <div class="container-narrow">
                            <div class="col-md-13">
                                @Html.DisplayFor(modelItem => item.WSSDAIA)

                            </div>
                        </div>
                    </td>
                    <td>
                        <div class="container-narrow">
                            <div class="col-md-13">
                                @Html.DisplayFor(modelItem => item.MoAIW)

                            </div>
                        </div>
                    </td>
                    <td>
                        <div class="container-narrow">
                            <div class="col-md-13">
                                @Html.DisplayFor(modelItem => item.Specific_Gravity)

                            </div>
                        </div>
                    </td>
                    <td>
                        <div class="container-narrow">
                            <div class="col-md-13">
                                @Html.DisplayFor(modelItem => item.Apparent_Specific_Gravity)

                            </div>
                        </div>
                    </td>
                    <td>
                        <div class="container-narrow">
                            <div class="col-md-13">
                                @Html.DisplayFor(modelItem => item.Water_Absorption_of_Aggregate)

                            </div>
                        </div>
                    </td>
                    <td>
                        <div class="container-narrow">
                            <div class="col-md-13">
                                @Html.DisplayFor(modelItem => item.Location.District)
                            </div>
                        </div>
                    </td>
                    <td>
                        <div class="container-narrow">
                            <div class="col-md-13">
                                <input type="submit" value="Edit" class="btn btn-warning col-lg-3 col-lg-offset-0" style="width:70px; height:35px;" onclick="location.href='@Url.Action("Edit",new {id = item.ID})'" />
                                @*@Html.ActionLink("Edit", "Edit", new { id = item.ID})*@
                            </div>
                        </div>
                    </td>
                    <td>
                        <div class="container-narrow">
                            <div class="col-md-13">
                                <input type="submit" value="Details" class="btn btn-success col-lg-3 col-lg-offset-0" style="width:70px; height:35px;" onclick="location.href='@Url.Action("Details",new {id = item.ID})'" />
                                @*@Html.ActionLink("Details", "Details", new { id = item.ID })*@
                            </div>
                        </div>
                    </td>
                    <td>
                        <div class="container-narrow">
                            <div class="col-md-13">
                                <input type="submit" value="Delete" class="btn btn-danger col-lg-3 col-lg-offset-0" style="width:70px; height:35px;" onclick="location.href='@Url.Action("Delete",new {id = item.ID})'" />
                                @*@Html.ActionLink("Delete", "Delete", new { id = item.ID })*@
                            </div>
                        </div>
                    </td>
                </tr>
            }
        </tbody>
    </table>





































